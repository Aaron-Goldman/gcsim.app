    "itty-router": "^2.6.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.11.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-typescript": "^3.0.0",
    "prettier": "^2.6.2",
    "typescript": "^4.7.3",
    "wrangler": "^2.0.9"
  }
